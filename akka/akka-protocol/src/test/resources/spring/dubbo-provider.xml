<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:protocol name="dubbo" port="20881"/>
    <dubbo:service  interface="net.dubboclub.akka.protocol.service.MyFirstDubboService"
                   ref="myFirstDubboService"/>
    <dubbo:service interface="net.dubboclub.akka.protocol.service.MyFirstDubboService"
                   ref="myFirstDubboService" version="1.0.1" path="dubboService"/>
    <dubbo:service interface="net.dubboclub.akka.protocol.service.MyFirstDubboService"
                   ref="myFirstDubboService" version="1.0.1" group="marster"/>
    <dubbo:service interface="net.dubboclub.akka.protocol.service.MyFirstDubboService"
                   ref="myFirstDubboService" version="*" group="*"/>
</beans>